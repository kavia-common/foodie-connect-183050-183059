# PUBLIC_INTERFACE
# Foodie Connect - Environment Variables (copy to .env and adjust)
REACT_APP_APP_NAME=Foodie Connect
REACT_APP_BASE_API_URL=http://localhost:4000
REACT_APP_WS_URL=
REACT_APP_WS_PATH=/socket.io

# Payments
REACT_APP_PAYMENT_PROVIDER=mock
REACT_APP_RAZORPAY_KEY=
REACT_APP_STRIPE_PUBLISHABLE_KEY=
REACT_APP_PAYTM_MERCHANT_ID=
REACT_APP_PAYMENT_SANDBOX=true

# Push notifications
REACT_APP_VAPID_PUBLIC_KEY=
REACT_APP_PUSH_ENABLED=false

# Maps
REACT_APP_MAPS_PROVIDER=placeholder
REACT_APP_MAPS_API_KEY=

# i18n
REACT_APP_DEFAULT_LOCALE=en
REACT_APP_SUPPORTED_LOCALES=en,hi

# Mocks
REACT_APP_ENABLE_MOCKS=true
